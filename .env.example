# Web Scraper Settings
# Copy this file to .env and customize it!
# (The .env file won't be tracked by git, so your settings stay private)

# Browser settings
HEADLESS=false
BROWSER_WIDTH=1280
BROWSER_HEIGHT=720

# Default wait time (milliseconds)
WAIT_TIME=2000

# Screenshot settings
SAVE_SCREENSHOTS=false
SCREENSHOT_DIR=screenshots

# Optional: If you need to scrape sites that require login
# USERNAME=your-username
# PASSWORD=your-password
